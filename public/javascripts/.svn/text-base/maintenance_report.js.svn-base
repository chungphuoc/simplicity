var theDiv = Array;

function updatePlaceVisibility() {
	popup  = document.getElementById("maintenance_request_place_list_item_id");
	theDiv[0] = document.getElementById("other_place_div1");
	theDiv[1] = document.getElementById("other_place_div2");
	if ( popup.value == "" || popup.value=="-1") {
		theDiv[0].style.display="block";
		theDiv[1].style.display="block";
	} else {
		theDiv[0].style.display="none";
		theDiv[1].style.display="none";
	}
}